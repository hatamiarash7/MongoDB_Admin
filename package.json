{
  "name": "mongo-admin",
  "version": "0.1.0",
  "description": "A Web GUI to administer MongoDB",
  "author": "Arash Hatami <hatamiarash7@gmail.com>",
  "private": false,
  "main": "electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hatamiarash7/MongoDB_Admin"
  },
  "bugs": {
    "url": "https://github.com/hatamiarash7/MongoDB_Admin/issues"
  },
  "scripts": {
    "start": "node app.js",
    "test": "mocha -R spec --bail --exit ./tests/tests.js",
    "packageOsx": "electron-packager ./ MongoDB_Admin --out=releases/ --platform=darwin --arch=x64 --prune --overwrite --ignore=releases/* --icon=public/logo.icns",
    "packageWin32": "electron-packager ./ MongoDB_Admin --out=releases/ --platform=win32 --arch=ia32 --prune --overwrite --ignore=releases/* --icon=public/logo.ico",
    "packageWin64": "electron-packager ./ MongoDB_Admin --out=releases/ --platform=win32 --arch=x64 --prune --overwrite --ignore=releases/* --icon=public/logo.ico",
    "packageLinux": "electron-packager ./ MongoDB_Admin --out=releases/ --platform=linux --arch=ia32 --prune --overwrite --ignore=releases/* --icon=public/logo.ico",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "admin",
    "web",
    "ui",
    "UI",
    "gui",
    "GUI",
    "DB",
    "database",
    "user",
    "manage",
    "manager",
    "administer",
    "interface",
    "document",
    "collection",
    "express",
    "electron",
    "open source",
    "mongo-admin",
    "mongoadmin"
  ],
  "dependencies": {
    "async": "^2.6.3",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-handlebars": "^5.3.1",
    "express-session": "^1.16.2",
    "font-awesome": "^4.6.3",
    "hbs": "^4.0.0",
    "i18n-2": "^0.6.3",
    "jquery": "^3.5.0",
    "junk": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.14.1",
    "mongo-uri": "^0.1.2",
    "mongodb": "^3.1.13",
    "mongodb-backup": "^1.6.4",
    "mongodb-extended-json": "^1.7.0",
    "mongodb-restore": "^1.5.4",
    "morgan": "~1.9.0",
    "nconf": "^0.8.4",
    "ncp": "^2.0.0",
    "nedb": "^1.8.0",
    "node-delete": "^0.1.2",
    "require.js": "^1.0.0",
    "serve-favicon": "~2.4.3"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "colors": "^1.4.0",
    "electron-prebuilt": "^1.2.8",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "jsdom": "^9.4.1",
    "mocha": "^6.2.0",
    "mocha-jsdom": "~1.1.0",
    "supertest": "^4.0.2"
  }
}
